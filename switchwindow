// nightwatch tests/t.js

module.exports = {

  'Demo test Google' : function (browser) {
    browser
    .url('https://www.google.com')
    .execute(function testing1() {
            window.open();
            window.open();
            window.open();
    }, [],
    (result)=>{
        console.log('you: ' + result.value);

  });

  },

//https://nightwatchjs.org/api/switchWindow.html

 'log' : function (browser) {
   browser.windowHandles(function(result) {
     // An array of window handles.
     console.log(result.value);
     var handle = result.value[2];
     browser.switchWindow(handle);
   });
 
   browser.pause(11000);
}


};
